<h1 class="text-center mb-4">Your Entries</h1>
<div class="container">
  <div class="row">
    <% @entries.each do |entry| %>
      <div class="col-md-3 mb-4">
        <div class="book">
          <div class="book-content">
            <h5 class="book-title"><%= entry.name %></h5>
            <p class="book-link">
              <a href="<%= entry.link %>">View Link</a>
            </p>
            <%= button_to "Delete", entry, method: :delete, class: "btn btn-danger btn-sm", data: { confirm: "Are you sure you want to delete this Project?" } %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>